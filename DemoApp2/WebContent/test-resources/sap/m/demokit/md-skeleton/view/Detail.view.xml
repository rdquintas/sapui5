<mvc:View
	controllerName="sap.ui.demo.mdskeleton.view.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page
		navButtonPress="onNavBack"
		title="{i18n>detailTitle}">
		<content>
			<ObjectHeader
				title="{Name}"
				number="{Price}"
				numberUnit="USD">
			</ObjectHeader>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>